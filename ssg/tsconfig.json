{
  "extends": "@quasar/app/tsconfig-preset",
  "compilerOptions": {
    "baseUrl": ".",
    "module": "commonjs",
    "typeRoots": [
      "node_modules/@types",
      "./typings"
    ],
    "esModuleInterop": true,
    "types": [
      "quasar"
    ],
    "noUncheckedIndexedAccess": true
  },
  "exclude": [
    "node_modules",
    ".quasar",
    "dist",
    "public",
    "src-cordova"
  ],
  "ts-node": {
    "files": true
  }
}