{
  "name": "yentis.github.io",
  "version": "2.27.0",
  "description": "Yentis' static site",
  "productName": "yentis.github.io",
  "author": "Yentis#5218",
  "private": true,
  "scripts": {
    "lint": "eslint -c ./eslint.config.js \"./src*/**/*.{ts,cjs,mjs,vue}\"",
    "build:android": "vue-tsc && node --max-old-space-size=8192 node_modules/@quasar/cli/bin/quasar build -m capacitor -T android",
    "build:ios": "vue-tsc && node --max-old-space-size=8192 node_modules/@quasar/cli/bin/quasar build -m capacitor -T ios",
    "sign:android": "apksigner sign --verbose --ks my-release-key.keystore --ks-key-alias manga_reader ./dist/capacitor/android/apk/release/app-release-unsigned.apk",
    "build:electron": "vue-tsc && node --max-old-space-size=8192 node_modules/@quasar/cli/bin/quasar build -m electron",
    "build:ssg": "vue-tsc && quasar ssg generate",
    "serve:ssg": "quasar ssg serve dist/ssg",
    "dev:ssg": "quasar ssg dev",
    "postinstall": "quasar prepare"
  },
  "@comments": {
    "devDependencies": {
      "@babel/core": "Peer dependency for @babel/eslint-parser",
      "@types/node-fetch": "Peer dependency for dropbox",
      "buffer": "Peer dependency for quasar-app-extension-ssg > crc",
      "jsdom": "Peer dependency for jsdom-global",
      "typescript": "Peer dependency for @typescript-eslint/eslint-plugin > tsutils",
      "webpack": "Peer dependency for node-polyfill-webpack-plugin"
    }
  },
  "dependencies": {
    "@capacitor/core": "^8.0.1",
    "@capacitor/local-notifications": "^8.0.0",
    "@ghostery/adblocker-electron": "2.13.4",
    "@quasar/cli": "2.5.0",
    "@quasar/extras": "1.17.0",
    "@vueuse/rxjs": "^14.1.0",
    "@zip.js/zip.js": "^2.7.24",
    "core-js": "^3.6.5",
    "dropbox": "^10.28.0",
    "isomorphic-fetch": "^2.2.1",
    "moment": "^2.29.2",
    "node-polyfill-webpack-plugin": "^1.1.3",
    "p-queue": "^6.6.2",
    "qs": "^6.14.1",
    "quasar": "2.18.6",
    "relevancy": "^0.2.0",
    "rxjs": "^7.8.2",
    "vue": "3",
    "vue-router": "4"
  },
  "devDependencies": {
    "@electron/packager": "^18.3.2",
    "@eslint/js": "^9.39.1",
    "@quasar/app-webpack": "4.3.1",
    "@types/chrome": "^0.1.33",
    "@types/isomorphic-fetch": "0.0.35",
    "@types/node": "^24.10.1",
    "@types/node-fetch": "^2.5.7",
    "@types/qs": "^6",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.6.0",
    "autoprefixer": "^10.4.22",
    "buffer": "^6.0.3",
    "cordova-plugin-inappbrowser": "^5.0.0",
    "devtron": "^1.4.0",
    "electron": "39.2.3",
    "electron-debug": "^3.1.0",
    "electron-devtools-installer": "^3.1.1",
    "electron-packager": "15.5.2",
    "eslint": "^9.16.0",
    "eslint-plugin-vue": "^10.6.0",
    "eslint-webpack-plugin": "^5.0.2",
    "globals": "^16.5.0",
    "jsdom": "^25.0.1",
    "jsdom-global": "^3.0.2",
    "prettier": "3",
    "quasar-app-extension-ssg": "^5.2.0",
    "ts-loader": "^9.4.2",
    "typescript": "5.9.2",
    "vue-eslint-parser": "^10.2.0",
    "vue-tsc": "^3.2.2"
  },
  "browserslist": [
    "last 10 Chrome versions",
    "last 10 Firefox versions",
    "last 4 Edge versions",
    "last 7 Safari versions",
    "last 8 Android versions",
    "last 8 ChromeAndroid versions",
    "last 8 FirefoxAndroid versions",
    "last 10 iOS versions",
    "last 5 Opera versions"
  ],
  "engines": {
    "node": ">= 25.0.0",
    "npm": ">= 11.6.2"
  },
  "type": "module",
  "packageManager": "yarn@4.11.0"
}
